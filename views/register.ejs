<html>
<head>
  <title>REGISTER YOUR SMART SELF</title>
  <link rel="stylesheet" href="/assets/css/styles.css">
  <script>window.INITIAL_STATE = <%-JSON.stringify(data)%>;</script>
</head>
<body>
  <% include ../static/assets/includes/header.ejs %>
  <div id="container">
    <div id="form">
      <h1>REGISTER YOUR SMART SELF</h1>
<form action="/register" method="post">
  <input type="text" placeholder="Username" name="username" autofocus required /><br />
  <input type="password" placeholder="Password" name="password" required /><br />
  <input type="text" placeholder="Name (optional)" name="fullName" optional /><br />
  <input type="text" placeholder="City (optional)" name="city" optional /><br />
  <input type="text" placeholder="State (optional)" name="state" optional /><br />
  <%if(data.warning===true){%><span id="warning">Invalid. Try again.</span><%}%><br />
  <input type="submit"><button id="submit">JOIN US</button></input>
</form>
  <a href="/login">log in</a></div>
</div>
<% include ../static/assets/includes/demoFooter.ejs %>
</body>
</html>
